{
  "name": "ergebnis/phpstan-rules",
  "description": "Provides additional rules for phpstan/phpstan.",
  "license": "MIT",
  "type": "phpstan-extension",
  "keywords": [
    "phpstan",
    "phpstan-rules",
    "phpstan-extreme-rules"
  ],
  "authors": [
    {
      "name": "Andreas MÃ¶ller",
      "email": "am@localheinz.com"
    }
  ],
  "homepage": "https://github.com/ergebnis/phpstan-rules",
  "support": {
    "issues": "https://github.com/ergebnis/phpstan-rules/issues",
    "source": "https://github.com/ergebnis/phpstan-rules"
  },
  "require": {
    "php": "~8.0.0 || ~8.1.0",
    "ext-mbstring": "*",
    "nikic/php-parser": "^4.2.3",
    "phpstan/phpstan": "^1.0.0"
  },
  "require-dev": {
    "doctrine/orm": "^2.12.1",
    "ergebnis/composer-normalize": "^2.19.0",
    "ergebnis/license": "^1.1.0",
    "ergebnis/php-cs-fixer-config": "^4.6.0",
    "nette/di": "^3.0.13",
    "phpstan/phpstan-deprecation-rules": "^1.0.0",
    "phpstan/phpstan-strict-rules": "^1.1.0",
    "phpunit/phpunit": "^8.5.26",
    "psalm/plugin-phpunit": "~0.16.1",
    "psr/container": "^1.0.0",
    "vimeo/psalm": "^4.22"
  },
  "autoload": {
    "psr-4": {
      "Ergebnis\\PHPStan\\Rules\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ergebnis\\PHPStan\\Rules\\Test\\": "test/"
    }
  },
  "config": {
    "allow-plugins": {
      "ergebnis/composer-normalize": true,
      "infection/extension-installer": true
    },
    "platform": {
      "php": "8.0.27"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "phpstan": {
      "includes": [
        "rules.neon"
      ]
    }
  }
}
